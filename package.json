{
  "name": "news-rag",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "dev": "next dev --turbo",
    "dev:full": "npm run chroma:start && npm run dev",
    "chroma:start": "echo 'Starting ChromaDB server...' && echo 'Run: pip install chromadb && chroma run --host localhost --port 8000'",
    "chroma:docker": "docker run -d -p 8000:8000 chromadb/chroma",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@chroma-core/default-embed": "^0.1.8",
    "@google/generative-ai": "^0.7.1",
    "@prisma/client": "^5.6.0",
    "@supabase/supabase-js": "^2.38.0",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-query": "^5.8.0",
    "axios": "^1.6.0",
    "chromadb": "^3.0.12",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "compromise": "^14.10.0",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.263.0",
    "next": "^15.2.3",
    "openai": "^4.20.0",
    "prisma": "^5.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rss-parser": "^3.13.0",
    "tailwind-merge": "^2.0.0",
    "zod": "^3.24.2",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.0.15",
    "@types/node": "^20.14.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.2.3",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.15",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0"
